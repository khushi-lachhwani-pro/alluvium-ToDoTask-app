{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "es2017"
    ],
    "jsx": "react-native",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "baseUrl": "./src/",
    "paths": {
      "@api": [
        "services/api"
      ],
      "@hook/*": [
        "hook/*"
      ],
      "@routes": [
        "routes"
      ],
      "@container/*": [
        "containers/*"
      ],
      "@containers/*": [
        "containers/*"
      ],
      "@lib/*": [
        "components/lib/*"
      ],
      "@global": [
        "global"
      ],
      "@components/*": [
        "components/*"
      ],
      "@components": [
        "components"
      ],
      "@redux/*": [
        "redux/*"
      ],
      "@storage/*": [
        "storage/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@services/*": [
        "services/*"
      ],
      "@guards": [
        "guards"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@assets/*": [
        "assets/*"
      ]
    },
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
